prompt prompt
    match /[\/a-zA-Z0-9._-]+ ?(?:\(config[^)]*\))? ?[#>] ?$/

prompt basic
    match /> ?$/

prompt privileged
    match /# ?$/

prompt configure
    match /\(config[^)]*\)# ?$/

prompt user
    match /[Uu]sername/

prompt pass
    match /[Pp]assword: ?$/

# MACROS

macro begin_privileged
    send enable
    match user or pass

macro end_privileged
    send disable

macro begin_configure
    send configure terminal

macro end_configure
    send exit

macro disconnect
    send exit

# macro completion
#     send ?
